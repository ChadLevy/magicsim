{
  "name": "magicsim-master",
  "version": "1.0.0",
  "description": "Shadow priest tailored tool for simming stat weights in a fight-aware manner using SimulationCraft.",
  "node-main": "app.js",
  "main": "http://localhost:4000/",
  "window": {
    "toolbar": false,
    "resizable": false,
    "width": 300,
    "height": 400
  },
  "scripts": {
    "start": "nw",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fullbuild": "npm run build32 && npm run build64",
    "buildandrun32": "npm run build32 && npm run run32",
    "buildandrun64": "npm run build64 && npm run run64",
    "build64": "npm install --production && (robocopy . ..\\build\\magicsim-master\\win64\\package.nw\\ /IT /MIR) ^& IF %ERRORLEVEL% LEQ 1 exit 0",
    "build32": "npm install --production && (robocopy . ..\\build\\magicsim-master\\win32\\package.nw\\ /IT /MIR) ^& IF %ERRORLEVEL% LEQ 1 exit 0",
    "run64": "..\\build\\magicsim-master\\win64\\nw.exe",
    "run32": "..\\build\\magicsim-master\\win32\\nw.exe"
  },
  "dependencies": {
    "cartesian-product": "~2.1.2",
    "dom-parser": "~0.1.5",
    "node-7z": "~0.4.0",
    "q": "~1.4.1",
    "xmldom": "~0.1.27",
    "xpath": "~0.0.23",
    "locks": "~0.2.2",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "express-session": "~1.15.0",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "yargs": "~6.6.0"
  },
  "devDependencies": {
    "nw": "~0.20.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phroph/magicsim.git"
  },
  "chromium-args": "--enable-logging=stderr",
  "author": "phroph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phroph/magicsim/issues"
  },
  "homepage": "https://github.com/phroph/magicsim#readme"
}
